        </packages>
        <!--classes>
            <class name="io.github.epam.html.tests.debug.DebugTests"/>
        </classes-->
    </test>
    <listeners>
        <listener class-name="io.github.epam.testng.TestNGListener" />
        <listener class-name="io.github.epam.testng.RetryListenerClass" />
    </listeners>
</suite>
