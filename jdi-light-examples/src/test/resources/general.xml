<?xml version="1.0" encoding="WINDOWS-1251"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test suite" parallel="methods" thread-count="1">
    <listeners>
        <listener class-name="io.github.epam.testng.TestNGListener" />
    </listeners>
    <test name="Tests">
        <packages>
            <package name="io.github.epam.tests.epam"/>
            <package name="io.github.epam.tests.google"/>
            <package name="io.github.epam.tests.recommended"/>
            <package name="io.github.epam.tests.selenium"/>
            <package name="io.github.epam.tests.smart.path"/>
        </packages>
    </test>
    <test name="Root">
        <classes>
            <class name="io.github.epam.tests.RootTests"/>
        </classes>
    </test>
</suite>